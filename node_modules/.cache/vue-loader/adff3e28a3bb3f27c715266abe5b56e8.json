onst pillType = computed(() => {\n  if (props.type) {\n    return props.type\n  }\n\n  if (props.progress) {\n    if (props.progress >= 60) {\n      return 'success'\n    }\n    if (props.progress >= 40) {\n      return 'warning'\n    }\n\n    return 'danger'\n  }\n\n  return 'info'\n})\n\nconst pillIcon = computed(() => {\n  return {\n    success: mdiTrendingUp,\n    warning: mdiTrendingNeutral,\n    danger: mdiTrendingDown,\n    info: null\n  }[pillType.value]\n})\n\nconst pillText = computed(() => props.text ?? `${props.progress}%`)\n</script>\n\n<template>\n  <card-component\n    class=\"mb-6 last:mb-0\"\n    hoverable\n  >\n    <level>\n      <level type=\"justify-start\">\n        <user-avatar\n          class=\"w-12 h-12 md:mr-6\"\n          :username=\"name\"\n        />\n        <div class=\"text-center md:text-left\">\n          <h4 class=\"text-xl\">\n            {{ name }} <span class=\"text-gray-500 dark:text-gray-400\">@{{ login }}</span>\n          </h4>\n          <p class=\"text-gray-500 dark:text-gray-400\">\n            {{ date }}\n          </p>\n        </div>\n      </level>\n      <pill\n        :type=\"pillType\"\n        :text=\"pillText\"\n        :icon=\"pillIcon\"\n      />\n    </level>\n  </card-component>\n</template>\n"]}]}