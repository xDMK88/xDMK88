der.vue'\nimport JbButton from '@/components/JbButton.vue'\nimport JbButtons from '@/components/JbButtons.vue'\nimport BottomOtherPagesSection from '@/components/BottomOtherPagesSection.vue'\nimport TitledSection from '@/components/TitledSection.vue'\nimport TitleSubBar from '@/components/TitleSubBar.vue'\n\nconst titleStack = ref(['Admin', 'Forms'])\n\nconst selectOptions = [\n  { id: 1, label: 'Business development' },\n  { id: 2, label: 'Marketing' },\n  { id: 3, label: 'Sales' }\n]\n\nconst form = reactive({\n  name: 'John Doe',\n  email: 'john.doe@example.com',\n  phone: '',\n  department: selectOptions[0],\n  subject: '',\n  question: ''\n})\n\nconst customElementsForm = reactive({\n  checkbox: ['lorem'],\n  radio: 'one',\n  switch: ['one'],\n  file: null\n})\n\nconst submit = () => {\n  //\n}\n</script>\n\n<template>\n  <title-bar :title-stack=\"titleStack\" />\n  <hero-bar>Forms</hero-bar>\n\n  <main-section>\n    <title-sub-bar\n      :icon=\"mdiBallotOutline\"\n      title=\"Forms example\"\n    />\n    <card-component\n      title=\"Forms\"\n      :icon=\"mdiBallot\"\n      form\n      @submit.prevent=\"submit\"\n    >\n      <field label=\"Grouped with icons\">\n        <control\n          v-model=\"form.name\"\n          :icon=\"mdiAccount\"\n        />\n        <control\n          v-model=\"form.email\"\n          type=\"email\"\n          :icon=\"mdiMail\"\n        />\n      </field>\n\n      <field\n        label=\"With help line\"\n        help=\"Do not enter the leading zero\"\n      >\n        <control\n          v-model=\"form.phone\"\n          type=\"tel\"\n          placeholder=\"Your phone number\"\n        />\n      </field>\n\n      <field label=\"Dropdown\">\n        <control\n          v-model=\"form.department\"\n          :options=\"selectOptions\"\n        />\n      </field>\n\n      <divider />\n\n      <field\n        label=\"Question\"\n        help=\"Your question. Max 255 characters\"\n      >\n        <control\n          type=\"textarea\"\n          placeholder=\"Explain how we can help you\"\n        />\n      </field>\n\n      <divider />\n\n      <jb-buttons>\n        <jb-button\n          type=\"submit\"\n          color=\"info\"\n          label=\"Submit\"\n        />\n        <jb-button\n          type=\"reset\"\n          color=\"info\"\n          outline\n          label=\"Reset\"\n        />\n      </jb-buttons>\n    </card-component>\n  </main-section>\n\n  <titled-section>\n    Custom elements\n  </titled-section>\n\n  <main-section>\n    <card-component\n      title=\"Custom elements\"\n      :icon=\"mdiBallotOutline\"\n    >\n      <field\n        label=\"Checkbox\"\n        wrap-body\n      >\n        <check-radio-picker\n          v-model=\"customElementsForm.checkbox\"\n          name=\"sample-checkbox\"\n          :options=\"{ lorem: 'Lorem', ipsum: 'Ipsum', dolore: 'Dolore' }\"\n        />\n      </field>\n\n      <divider />\n\n      <field\n        label=\"Radio\"\n        wrap-body\n      >\n        <check-radio-picker\n          v-model=\"customElementsForm.radio\"\n          name=\"sample-radio\"\n          type=\"radio\"\n          :options=\"{ one: 'One', two: 'Two' }\"\n        />\n      </field>\n\n      <divider />\n\n      <field label=\"Switch\">\n        <check-radio-picker\n          v-model=\"customElementsForm.switch\"\n          name=\"sample-switch\"\n          type=\"switch\"\n          :options=\"{ one: 'One', two: 'Two' }\"\n        />\n      </field>\n\n      <divider />\n\n      <file-picker v-model=\"customElementsForm.file\" />\n    </card-component>\n  </main-section>\n\n  <bottom-other-pages-section />\n</template>\n"]}]}